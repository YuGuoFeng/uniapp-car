page {
  background-color: #F5F5F5;
}
.index .image_style {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .image_style .top_style {
  display: flex;
  justify-content: start;
  align-items: center;
}
.index .image_style .top_style .font_1 {
  font: 34rpx;
  margin-left: 30rpx;
}
.index .image_style .right {
  font-size: 24rpx;
  line-height: 40rpx;
  padding: 0 15rpx;
  border: 2rpx solid #808080;
  border-radius: 20rpx;
}
.index .register_style {
  height: 60rpx;
  line-height: 50rpx;
  border-bottom: 2rpx solid #DCDCDC;
}
.index .content_1 {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .content_1 .content {
  font-size: 34rpx;
  width: 50%;
  height: 200rpx;
  border-right: 2rpx solid #DCDCDC;
  text-align: center;
}
.index .content_1 .content .font_1 {
  margin-top: 50rpx;
  font-size: 50rpx;
  color: #FB954F;
}
.index .content_1 .content .font_style {
  margin-top: 10rpx;
  display: block;
}
.index .content_1 .right {
  font-size: 34rpx;
  width: 50%;
  height: 200rpx;
  text-align: center;
}
.index .content_1 .right .font_1 {
  margin-top: 50rpx;
  font-size: 60rpx;
  color: #20B923;
}
.index .content_1 .right .font_style {
  display: block;
}

