page {
  background-color: #F5F5F5;
  padding-bottom: 100rpx;
}
.top_1 {
  font-size: 36rpx;
  margin-top: 10rpx;
}
.top_2 {
  width: 100rpx;
  height: 30rpx;
  background-color: #CCCCCC;
  border-radius: 5px;
  text-align: center;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  margin: 20rpx 0;
}
.two_2 {
  display: flex;
  justify-content: start;
  align-items: center;
}
.two_2 .top_1 {
  font-size: 50rpx;
  color: #66A8FB;
}
.two_2 .font_style {
  font: 27rpx;
  color: #CCCCCC;
  margin-left: 10rpx;
}
.one_1 {
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 15rpx;
}
.mapStyle {
  padding: 0 30rpx;
  margin: 10px 0;
}
.button_style {
  width: 100%;
  margin: 35rpx 0;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.button_style .bottom_1 {
  background-color: #1AAD19 !important;
  color: #FFFFFF !important;
}
.content_1 {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content_1 .content {
  font-size: 34rpx;
  width: 50%;
  height: 200rpx;
  border-right: 2rpx solid #DCDCDC;
  text-align: center;
}
.content_1 .content .font_1 {
  margin-top: 50rpx;
  font-size: 50rpx;
  color: orange;
}
.content_1 .content .font_style {
  margin-top: 10rpx;
  display: block;
}
.content_1 .right {
  font-size: 34rpx;
  width: 50%;
  height: 200rpx;
  text-align: center;
}
.content_1 .right .font_1 {
  margin-top: 50rpx;
  font-size: 60rpx;
  color: #20B923;
}
.content_1 .right .font_style {
  display: block;
}
.font_botton {
  margin-top: 20rpx;
  padding: 0 60rpx;
  font-size: 28rpx;
  color: #787878;
}
.font_botton .font_one {
  margin: 10rpx;
}
.time_1 {
  width: 100%;
  margin: 150rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #787878;
}
.name_1 {
  text-align: center;
  font-size: 28rpx;
}
.banben {
